@model Product

<div class="card" style="width: 18rem;">
    <div class="card-body">
        <h5 class="card-title">Product Name: @Model.Name</h5>
        <h6 class="card-subtitle mb-2 text-muted">Product Age: @Model.Description</h6>
        <h6 class="card-subtitle mb-2 text-muted">Product Salary: @Model.Cost</h6>
        <h6 class="card-subtitle mb-2 text-muted">Product Address: @Model.Price</h6>
        <h6 class="card-subtitle mb-2 text-muted">Product Email: @Model.Count</h6>
        <h6 class="card-subtitle mb-2 text-muted">Product Email: @Model.ImageURL</h6>
        <h6 class="card-subtitle mb-2 text-muted">Category Name: @Model.Category!.Name</h6>
        <a asp-controller="Product" asp-action="Details" class="btn btn-primary mt-4">Back</a>
    </div>
</div>